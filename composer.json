{
    "name": "mjamilasfihani/indonesian-translation",
    "description": "Unofficial Indonesian's translation plugin for Laravel-Lang",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "languages",
        "lang",
        "translations",
        "messages"
    ],
    "authors": [
        {
            "name": "Mohammad Jamil Asfihani",
            "email": "mohammadjamilasfihani@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "illuminate/collections": "^11.0",
        "illuminate/support": "^11.0",
        "laravel-lang/publisher": "^16.0"
    },
    "require-dev": {
        "illuminate/console": "^11.0",
        "illuminate/container": "^11.0",
        "laravel-lang/actions": "^1.0",
        "laravel-lang/attributes": "^2.5",
        "laravel-lang/http-statuses": "^3.5",
        "laravel-lang/lang": "^15.10",
        "laravel-lang/status-generator": "^2.0",
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "JamilAsfi\\IndonesianTranslation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JamilAsfi\\IndonesianTranslation\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true
        },
        "prefered-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JamilAsfi\\IndonesianTranslation\\TranslationServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
